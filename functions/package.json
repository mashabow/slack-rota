{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "serve": "tsc-watch --onFirstSuccess 'firebase emulators:start --only functions,firestore'",
    "shell": "tsc-watch --onFirstSuccess 'firebase functions:shell'",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "eslint --format codeframe --ext .js,.jsx,.ts,.tsx .",
    "lint:ci": "npm run lint -- --max-warnings=0",
    "type": "tsc --noEmit",
    "test": "jest",
    "test:ci": "npm run test -- --ci"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@slack/bolt": "^3.6.0",
    "@speee-js/jsx-slack": "^2.4.0",
    "date-fns": "^2.15.0",
    "date-fns-tz": "^1.0.10",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1"
  },
  "devDependencies": {
    "@slack-wrench/jest-mock-web-client": "^1.2.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "firebase-functions-test": "^0.2.1",
    "jest": "^27.2.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^27.0.5",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.4.3"
  },
  "private": true
}
