{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "serve": "tsc-watch --onFirstSuccess 'firebase emulators:start --only functions,firestore'",
    "shell": "tsc-watch --onFirstSuccess 'firebase functions:shell'",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "eslint --format codeframe --ext .js,.jsx,.ts,.tsx .",
    "test": "jest"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@slack/bolt": "^2.2.3",
    "@speee-js/jsx-slack": "^2.4.0",
    "date-fns": "^2.15.0",
    "date-fns-tz": "^1.0.10",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "firebase-functions-test": "^0.2.0",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.7"
  },
  "private": true
}
