{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "serve": "tsc-watch --onFirstSuccess 'firebase emulators:start --only functions,firestore'",
    "shell": "tsc-watch --onFirstSuccess 'firebase functions:shell'",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --format codeframe --ext .js,.jsx,.ts,.tsx . --max-warnings=0",
    "lint:prettier": "prettier --check .",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "prettier --write .",
    "type": "tsc --noEmit",
    "test": "jest",
    "test:ci": "npm run test -- --ci"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@slack/bolt": "^3.6.0",
    "date-fns": "^2.15.0",
    "date-fns-tz": "^1.0.10",
    "firebase-admin": "^9.11.1",
    "firebase-functions": "^3.6.1",
    "jsx-slack": "^4.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^27.2.2",
    "@slack-wrench/jest-mock-web-client": "^1.3.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.26.0",
    "firebase-functions-test": "^0.2.1",
    "jest": "^27.2.1",
    "prettier": "^2.4.1",
    "supertest": "^4.0.2",
    "ts-jest": "^27.0.5",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.4.3"
  },
  "private": true
}
